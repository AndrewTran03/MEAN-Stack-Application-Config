{
  "name": "client",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "format": "prettier --write \"**/*.ts*\" \"**/*.js*\" \"**/*.css\" \"**/*.html\"",
    "test": "ng test",
    "deploy": "ng deploy",
    "cypress": "cypress open --browser chrome",
    "cypress:code-cov-report": "nyc report --reporter=html",
    "test:component": "cypress run --component --browser chrome --headed --no-exit",
    "test:e2e": "cypress run --e2e --browser chrome --headed --no-exit",
    "test:e2e-script": "./cypress/run_e2e_cypress_tests.sh 5500",
    "test:e2e-code-cov": "open ./coverage/client/index.html"
  },
  "private": true,
  "nyc" : {
    "all": true,
    "include": ["src/**/*.ts", "src/**/*.html"]
  },
  "dependencies": {
    "@angular/animations": "^17.0.0",
    "@angular/cdk": "^17.3.4",
    "@angular/common": "^17.0.0",
    "@angular/compiler": "^17.0.0",
    "@angular/core": "^17.0.0",
    "@angular/forms": "^17.0.0",
    "@angular/material": "^17.3.4",
    "@angular/platform-browser": "^17.0.0",
    "@angular/platform-browser-dynamic": "^17.0.0",
    "@angular/router": "^17.0.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.0.7",
    "@angular/cli": "^17.0.7",
    "@angular/compiler-cli": "^17.0.0",
    "@cypress/code-coverage": "^3.12.39",
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@jsdevtools/coverage-istanbul-loader": "^3.0.5",
    "@types/cypress__code-coverage": "^3.10.2",
    "@types/d3": "^7.4.3",
    "@types/jasmine": "~5.1.0",
    "autoprefixer": "^10.4.19",
    "axios": "^1.6.3",
    "cypress": "^13.8.1",
    "d3": "^7.8.5",
    "nyc": "^15.1.0",
    "postcss": "^8.4.38",
    "prettier": "3.1.1",
    "socket.io-client": "^4.7.2",
    "tailwindcss": "^3.4.3",
    "tslint-config-prettier": "1.18.0",
    "typescript": "~5.2.2",
    "zod": "^3.22.4",
    "zod-validation-error": "^2.1.0"
  }
}
